class HeavyPills 
{
	public static void main(String[] args) 
	{
		int b=19;
		int b1=20;
		for(int i=0;i<19;i++)
		{
		System.out.println("it contains 0.1grams "+i);
		}
		if(b1>b)
		{
			System.out.println("b1 is greater because it contains 1.1grams ");
		}
		else
		{
			System.out.println("b1 is not greater because it does not contains 1.1grams ");
		}
	}
}
